MACDATA <- read.csv("/Users/luisvalderrama/Desktop/HHMI Research/requeriesmac2311/updatedtofall16MAC2311.csv")

table(MACDATA$Max.Score_NA, useNA="always")

ls(MACDATA) ####Shows variables of data (Lists objects)
nrow(MACDATA) ###### number of rows
ncol(MACDATA) #### number of columns
table(MACDATA$Sex)


####SUMMARY FOR SAT SCORES

  ### CREATING VARIABLE
is.na(MACDATA$Max.Score) <- !MACDATA$Max.Score#### to rid 0 and convert them to NA
MACDATA$Max.Score_NA <- MACDATA$Max.Score
table(MACDATA$Max.Score_NA)  #### TO Check 0 were deleted

summary(MACDATA$Max.Score_NA)


describeBy(MACDATA$Max.Score_NA, MACDATA$Term, mat = TRUE) ### separated SAT max scores to term and did descr. stat.s

#colors = ("peachpuff","tomato","tan1","palegreen","seagreen""paleturquoise","rotalblue","plum2","peachpuff","tomato","tan1","palegreen","seagreen""paleturquoise","rotalblue","plum2")



## Instead of colors you can Hex numbers ###
boxplot1 <- ggplot(MACDATA, aes(x = as.factor(Term), y = Max.Score_NA, fill=as.factor(Term))) + geom_boxplot()+ labs(title= "SAT Math Scores MAC2311 Fall '11 to Fall '16", x="Semester", y="SAT Math Score") + theme(legend.position = "none") + scale_fill_manual(values = c("#5db7d4", "#5dd4cd","#5dd49c","#5db7d4", "#5dd4cd", "#5dd49c","#5db7d4","#5dd4cd","#5dd49c", "#5db7d4", "#5dd4cd","#5dd49c", "#5db7d4", "#5dd4cd","#5dd49c", "#5db7d4"))+
  scale_x_discrete(labels = c("Fall 2011", "Spring 2012", "Summer 2012", "Fall 2012", "Spring 2013", "Summer 2013", "Fall 2013", "Spring 2014", "Summer 2014", "Fall 2014", "Spring 2015", "Summer 2015", "Fall 2015", "Spring 2016","Summer 2016", "Fall 2016")) + theme(axis.text.x= element_text(colour= "darkblue", angle = 90, size = 10, vjust = 0.6), axis.text.y=element_text(colour = "darkblue", size = 12), title=element_text(colour = "Navy blue", size = 15))

