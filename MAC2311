### WESTED DATA ###
mydata=ldply(list.files(path="/Users/luisvalderrama/Desktop/HHMI Research/WESTED DATA/CSV",pattern="csv",full.names=TRUE),function(filename) {
  dum=read.csv(filename)
  dum$filename=filename
  return(dum)
})

